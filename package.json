{
	"name": "rambda-scripts",
	"scripts": {
		"piped": "node scripts/build-multiple-types/build-multiple-types piped",
		"benchmarks": "node scripts/run-benchmarks/run-benchmarks-spec.js",
		"benchmarksx": "node scripts/run-benchmarks/run-complex-benchmarks-spec.js",
		"build": "tsc -p tsconfig.json",
		"consume": "cd scripts/consume-typings&&yarn start",
		"github": "node scripts/github-release/github-release.js",
		"highlighter": "RAMBDA_SCRIPTS_MODE=highlighter jest src/apply-rambda-scripts.spec.ts",
		"immutable": "node scripts/immutable-lint/immutable-lint.js",
		"in": "tsc --watch",
		"lint": "yarn lint:rambda&&yarn lint:tests",
		"lint:rambda": "node scripts/lint/lint.js",
		"lint:tests": "node scripts/lint-ts-files/prepare.js&&node scripts/lint-ts-files/lint-typings-tests.js",
		"lintx": "cd src && run lint",
		"new": "node scripts/add-new-method/add-new-method",
		"populate:docs": "RAMBDA_SCRIPTS_MODE=populate:docs jest src/apply-rambda-scripts.spec.ts",
		"populate:docs:rambdax": "WITH_RAMBDAX=ON RAMBDA_SCRIPTS_MODE=populate:docs jest src/apply-rambda-scripts.spec.ts",
		"populate:readme": "RAMBDA_SCRIPTS_MODE=populate:readme jest src/apply-rambda-scripts.spec.ts",
		"populate:readme:rambdax": "WITH_RAMBDAX=ON RAMBDA_SCRIPTS_MODE=populate:readme jest src/apply-rambda-scripts.spec.ts",
		"summary": "jest src/read-benchmarks/benchmark-summary.spec.ts",
		"toolbelt": "RAMBDA_SCRIPTS_MODE=toolbelt jest src/apply-rambda-scripts.spec.ts",
		"usedby": "RAMBDA_SCRIPTS_MODE=usedby jest src/apply-rambda-scripts.spec.ts"
	},
	"main": "index.js",
	"version": "1.0.0",
	"dependencies": {
		"got": "11.8.1",
		"fs-extra": "9.0.1",
		"helpers-fn": "1.6.0",
		"rambdax": "7.1.0",
		"rambda": "6.5.1",
		"ramda": "0.27.1",
		"shiki": "0.2.7",
		"eslint-plugin-functional": "3.2.1",
		"eslint": "7.18.0",
		"@typescript-eslint/eslint-plugin": "4.14.0",
		"@typescript-eslint/parser": "4.14.0",
		"@types/ramda": "0.27.34",
		"jest": "26.6.3",
		"ts-jest": "26.4.4",
		"ts-loader": "8.0.14",
		"tslint-fn": "0.2.2",
		"typescript": "4.1.3",
		"string-fn": "3.1.0"
	},
	"devDependencies": {
		"@types/fs-extra": "9.0.6",
		"@types/jest": "26.0.20",
		"@types/node": "14.14.21"
	},
	"license": "MIT",
	"url": "git+https://github.com/selfrefactor/rambda-scripts.git"
}
