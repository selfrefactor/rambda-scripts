    "rambdax": "7.0.1",
    "string-fn": "3.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.3",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.6",
    "jest": "26.6.3",
    "shiki": "0.2.6",
    "ts-jest": "26.4.1",
    "ts-loader": "8.0.5",
    "tslint-fn": "0.2.2",
    "typescript": "4.0.5"
  },
