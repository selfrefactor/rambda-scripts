{
  "name": "rambda-scripts",
  "scripts": {
    "immutable": "node scripts/immutable-lint/immutable-lint.js",
    "hjson:parse": "node scripts/parse-hjson/parse-hjson.js",
    "hjson:fix": "node scripts/parse-hjson/fix-hjson.js",
    "lintx": "cd src && run lint",
    "in": "tsc --watch",
    "build": "tsc -p tsconfig.json",
    "consume": "cd scripts/consume-typings&&yarn start",
    "lint": "yarn lint:rambda&&yarn lint:tests",
    "lint:rambda": "node scripts/lint/lint.js",
    "lint:tests": "node scripts/lint-ts-files/prepare.js&&node scripts/lint-ts-files/lint-typings-tests.js",
    "highlighter": "RAMBDA_SCRIPTS_MODE=highlighter jest src/apply-rambda-scripts.spec.ts",
    "usedby": "RAMBDA_SCRIPTS_MODE=usedby jest src/apply-rambda-scripts.spec.ts",
    "toolbelt": "RAMBDA_SCRIPTS_MODE=toolbelt jest src/apply-rambda-scripts.spec.ts"
  },
  "main": "index.js",
  "version": "1.0.0",
  "dependencies": {
    "got": "11.8.0",
    "fs-extra": "^9.0.1",
    "helpers-fn": "1.0.0",
    "rambdax": "7.1.0",
    "shiki": "0.2.6",
    "eslint-plugin-functional": "3.1.0",
    "eslint": "7.13.0",
    "@typescript-eslint/eslint-plugin": "4.8.1",
    "@typescript-eslint/parser": "4.8.1",
    "jest": "26.6.3",
    "ts-jest": "26.4.1",
    "ts-loader": "8.0.5",
    "tslint-fn": "0.2.2",
    "typescript": "4.0.5",
    "string-fn": "3.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.3",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.6"
  },
  "license": "MIT",
  "url": "git+https://github.com/selfrefactor/rambda-scripts.git"
}
